{
	"name": "webmscore-svelte-pwa",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "npm run vite-workaround && npm run smui-theme-light && npm run smui-theme-dark",
		"vite-workaround": "find node_modules/@material/ -name \"*.js.map\" -type f -delete",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"type": "module",
	"dependencies": {
		"@material/theme": "^12.0.0",
		"@material/typography": "^12.0.0",
		"@smui/banner": "^4.2.0",
		"@smui/button": "^4.2.0",
		"@smui/card": "^4.2.0",
		"@smui/checkbox": "^4.2.0",
		"@smui/circular-progress": "^4.2.0",
		"@smui/form-field": "^4.2.0",
		"@smui/icon-button": "^4.2.0",
		"@smui/linear-progress": "^4.2.0",
		"@smui/list": "^4.2.0",
		"@smui/radio": "^4.2.0",
		"@smui/select": "^4.2.0",
		"@smui/slider": "^4.2.0",
		"@smui/snackbar": "^4.2.0",
		"@smui/textfield": "^4.2.0",
		"@smui/tooltip": "^4.2.0",
		"@smui/top-app-bar": "^4.2.0",
		"@sveltejs/adapter-static": "^1.0.0-next.16",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.29.0",
		"@typescript-eslint/parser": "^4.29.0",
		"browser-fs-access": "^0.20.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"jszip": "^3.7.1",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.3.1",
		"sass": "^1.37.5",
		"smui-theme": "^4.2.0",
		"svelte": "^3.42.1",
		"svelte-check": "^2.2.4",
		"svelte-preprocess": "^4.7.4",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5",
		"vite": "^2.4.4",
		"webmscore": "^0.21.0"
	}
}
