{
	"name": "webmscore-pwa",
	"version": "1.1.0",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"type": "module",
	"dependencies": {
		"@material/theme": "^13.0.0",
		"@material/typography": "^13.0.0",
		"@smui/banner": "^v6.0.0-beta.15",
		"@smui/button": "^v6.0.0-beta.15",
		"@smui/card": "^v6.0.0-beta.15",
		"@smui/checkbox": "^v6.0.0-beta.15",
		"@smui/circular-progress": "^v6.0.0-beta.15",
		"@smui/form-field": "^v6.0.0-beta.15",
		"@smui/icon-button": "^v6.0.0-beta.15",
		"@smui/linear-progress": "^v6.0.0-beta.15",
		"@smui/list": "^v6.0.0-beta.15",
		"@smui/radio": "^v6.0.0-beta.15",
		"@smui/select": "^v6.0.0-beta.15",
		"@smui/slider": "^v6.0.0-beta.15",
		"@smui/snackbar": "^v6.0.0-beta.15",
		"@smui/tab": "^6.0.0-beta.15",
		"@smui/tab-bar": "^6.0.0-beta.15",
		"@smui/textfield": "^v6.0.0-beta.15",
		"@smui/tooltip": "^v6.0.0-beta.15",
		"@smui/top-app-bar": "^v6.0.0-beta.15",
		"@sveltejs/adapter-static": "^1.0.0-next.16",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"browser-fs-access": "^0.25.0",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"jszip": "^3.7.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"sass": "1.49.9",
		"smui-theme": "^6.0.0-beta.15",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.5",
		"svelte-material-ui": "^6.0.0-beta.15",
		"svelte-preprocess": "^4.10.4",
		"sveltekit-i18n": "^2.1.1",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2",
		"vite": "^2.8.6",
		"webmscore": "^0.21.0"
	}
}
